#!/bin/bash

export MITIE_MODELS=../../../MITIE-models

go()
{
    THREADS="--threads=16"
    LOG=test_$1_$2.txt
    #./ner --train-chunker eng.train_all $THREADS > $LOG
    ./ner --train-id -C$1     eng.train_all $THREADS >> $LOG
    ./ner --test-id eng.testb >> $LOG
    ./ner --tag-conll-file ner_model.dat eng.testb | ./conlleval 
    echo $LOG
    tail -n1 $LOG 
}



run_default_params()
{
    THREADS="--threads=16"
    LOG=test_default.txt
    ./ner --train-chunker eng.train_all $THREADS > $LOG
    ./ner --train-id      eng.train_all $THREADS >> $LOG
    ./ner --test-id eng.testb >> $LOG
    ./ner --tag-conll-file ner_model.dat eng.testb | ./conlleval 
    echo $LOG
    tail -n1 $LOG 
}

run_default_params
